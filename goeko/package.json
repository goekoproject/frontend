{
  "name": "@goeko/source",
  "version": "1.1.5",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "bun run start.js",
    "build:dev": "concurrently  -c \"blue,green\" \"npx nx run-many --target=build --projects=goeko --configuration=development \" \"npm run start-tailwind:build\"",
    "build:pro": "concurrently  -c \"blue,green\"  \"npx nx run-many --target=build --projects=goeko --configuration=production\" \"npm run start-tailwind:build\"",
    "start-tailwind": "npx tailwindcss -i ./apps/goeko/src/tailwind.scss -o ./dist/tailwind-styles.scss --watch",
    "start-tailwind:build": "npx tailwindcss -i ./apps/goeko/src/tailwind.scss -o ./dist/tailwind-styles.scss --minify",
    "lint": "nx lint",
    "prestart": "bun run prestart.js",
    "nx-reset": "nx reset",
    "test": "nx test goeko  --codeCoverage=true --test-file",
    "lighthouse": "npx unlighthouse --site https://goeko.ch/login",
    "lighthouse:dev": "npx unlighthouse --site https://platform.goeko.ch/",
    "demo": "npm run build:dev && npx firebase hosting:channel:deploy goekohome --expires 7d",
    "e2e": "nx e2e goeko-e2e --watch",
    "deploy:dev": "npm run build:dev && firebase deploy --only hosting:dev -P goeko-dev",
    "deploy:pro": "npm run build:pro && firebase deploy --only hosting:legacy -P goeko-pro",
    "deploy:ch": "npm run build:pro && firebase deploy --only hosting:ch -P goeko-pro",
    "deploy:eu": "npm run build:pro && firebpase deploy --only hosting:eu -P goeko-pro"
  },
  "private": true,
